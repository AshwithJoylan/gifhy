{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": [
          "./src"
        ],
        "extensions": [
          ".js",
          ".ts",
          ".tsx",
          ".ios.js",
          ".android.js"
        ],
        "alias": {
          "@features": "./src/features",
          "@components": "./src/components",
          "@utils": "./src/utils",
          "@navigation": "./src/navigation",
          "@screens": "./src/screens",
          "@types": "./src/types"
        }
      }
    ]
  ]
}